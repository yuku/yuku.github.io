    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/mdx": "^1.6.22",
    "@types/classnames": "^2.2.11",
    "@types/glob": "^7.1.3",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.3",
    "@types/rss": "^0.0.28",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "@zeit/next-sass": "^1.0.1",
    "babel-loader": "^8.2.2",
