    "next": "^10.0.9",
    "next-ga": "^2.3.4",
    "node-sass": "^5.0.0",
    "notebookjs": "^0.6.3",
    "null-loader": "^4.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rehype-katex": "^4.0.0",
    "rehype-parse": "^7.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-footnotes": "^3.0.0",
